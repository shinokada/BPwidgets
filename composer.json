{
  "name": "shinokada/bpwidgets",
  "description": "This library is for Laravel-Backpack. Title, position, order and content will be saved in the database.",
  "license": "MIT",
  "authors": [
    {
      "name": "Shinichi Okada",
      "email": "okada.shin@gmail.com",
      "homepage": "https://github.com/shinokada/bpwidgets",
      "role": "Developer"
    }
  ],
  "homepage": "https://github.com/shinokada/bpwidgets",
  "keywords": [
    "Laravel",
    "BPwidgets",
    "Laravel-Backpack"
  ],
  "require": {
    "illuminate/support": "~5",
    "backpack/crud": "^3.2.0",
    "php": "~7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6.0",
    "orchestra/testbench": "~3.0"
  },
  "autoload": {
    "psr-4": {
      "shinokada\\BPwidgets\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "shinokada\\BPwidgets\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "shinokada\\BPwidgets\\BPwidgetsServiceProvider"
      ],
      "aliases": {
        "BPwidgets": "shinokada\\BPwidgets\\Facades\\BPwidgets"
      }
    }
  }
}
